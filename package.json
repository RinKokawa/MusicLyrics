{
  "name": "music-lyrics",
  "private": true,
  "version": "1.0.0",
  "description": "MusicLyrics - Vue 3 + TypeScript + Electron",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "electron": "electron .",
    "electron:dev": "vite build && electron .",
    "parse": "node lyrics-parser.js",
    "simple": "node simple-parser.js",
    "build": "vue-tsc && vite build && electron-builder",
    "build:win": "vue-tsc && vite build && electron-builder --win",
    "build:mac": "vue-tsc && vite build && electron-builder --mac",
    "build:linux": "vue-tsc && vite build && electron-builder --linux",
    "pack": "vue-tsc && vite build && electron-builder --dir",
    "dist": "vue-tsc && vite build && electron-builder",
    "package": "node scripts/build.js",
    "package:win": "node scripts/build.js win",
    "package:mac": "node scripts/build.js mac",
    "package:linux": "node scripts/build.js linux",
    "preview": "vite preview",
    "build:simple": "node scripts/build-simple.js"
  },
  "dependencies": {
    "vue": "^3.4.21",
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.4",
    "typescript": "^5.2.2",
    "vite": "^5.1.6",
    "vue-tsc": "^2.0.26",
    "electron": "^30.0.1",
    "electron-builder": "^24.13.3",
    "vite-plugin-electron": "^0.28.6",
    "vite-plugin-electron-renderer": "^0.14.5"
  },
  "main": "dist-electron/main.js",
  "keywords": ["vue", "electron", "typescript", "music", "lyrics", "desktop-app"],
  "author": "RinKokawa",
  "license": "MIT"
}
